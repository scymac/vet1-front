		"eslint-config-airbnb": "^19.0.4",
		"eslint-plugin-unused-imports": "^2.0.0",
		"html2canvas": "^1.4.1",
		"jspdf": "^3.0.3",
		"react": "^18.3.1",
		"react-alert": "^7.0.3",
		"react-dom": "^18.3.1",
		"react-modal": "^3.16.3",
